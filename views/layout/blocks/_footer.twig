<footer class="footer">
	<div class="container">
		<div class="sponsors">
			{% for i in sponsors %}
				<a href="#">
					<img src="img/{{i.sponsor}}" width="{{i.width}}" height="{{i.height}}" alt="image description">
				</a>
			{% endfor %}
		</div>
		<nav class="footer-nav">
			<ul class="footer-menu">
				{% for i in items %}
					<li>
						<a href="#">{{i.text}}</a>
					</li>
				{% endfor %}
			</ul>
		</nav>
		<p>&copy; 2013 Visit Burbank / Burbank Hospitality Association. All Rights Reserved.</p>
	</div>
</footer>