		</div>
		<div class="popular-block">
			<h4>Popular Must See</h4>
			<div class="accordion">
				{% for i in titles %}
					<h5 {% if loop.last %}data{% endif %}>{{i.titleH}}</h5>
					<div class="accord-block">
						<div class="img-block">
							<div class="img-holder">
								<img {% if loop.last %}data{% endif %} src="{{i.img}}" width="136"  heigth="127" alt="image description">
							</div>
						</div>
						<div class="text-block">
							<div {% if loop.last %}data{% endif %} class="title">{{i.title}}</div>
							<p {% if loop.last %}data{% endif %}>{{i.description}}</p>
						</div>
					</div>
				{% endfor %}
			</div>
		</div>
	</div>
