<div class="popular">
	<div class="container">
		<div class="see-block">
			<ul class="one-time">
				<li>
					<div class="title-img"></div>
					<div class="img-holder">
						<img src="img/popular-slide-1.png" width="409"  heigth="260" alt="image description">
					</div>
					<div class="text-block">
